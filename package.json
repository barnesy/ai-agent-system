{
  "name": "ai-agent-system",
  "version": "0.1.0",
  "description": "Modular AI agent system for enhanced human-AI collaboration",
  "main": "dist/index.js",
  "bin": {
    "ai-agent": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:unit": "jest src",
    "test:integration": "jest tests/integration",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "demo": "npm run build && node dist/examples/workflow-demo.js",
    "example:bugfix": "npm run build && node dist/workflows/bug-fix-workflow.js",
    "example:feature": "npm run build && node dist/workflows/feature-development-workflow.js",
    "example:review": "npm run build && node dist/workflows/code-review-workflow.js",
    "demo:metrics": "npm run build && node dist/demos/metrics-demo.js",
    "demo:memory": "npm run build && node dist/demos/memory-demo.js",
    "demo:ai": "npm run build && node dist/demos/ai-provider-demo.js",
    "demo:workflow": "npm run build && node dist/demos/workflow-template-demo.js",
    "demo:visual": "node scripts/run-visual-demo.js",
    "demo:visual:standalone": "open src/visual/demos/standalone-demo.html",
    "build:visual": "tsc --project tsconfig.visual.json",
    "test:notify": "./scripts/test-notify.sh",
    "validate": "npm run build && node dist/scripts/validate-structure.js",
    "precommit": "npm run validate && npm run lint && npm run typecheck && npm run test",
    "phase1": "AI_AGENT_PHASE=1 node dist/cli/phase1-cli.js",
    "phase1:build": "tsc --project tsconfig.phase1.json",
    "phase1:test": "./scripts/test-phase1.sh",
    "phase1:research": "AI_AGENT_PHASE=1 node dist/cli/phase1-cli.js research",
    "phase1:implement": "AI_AGENT_PHASE=1 node dist/cli/phase1-cli.js implement",
    "phase1:status": "AI_AGENT_PHASE=1 node dist/cli/phase1-cli.js status",
    "phase1:metrics": "AI_AGENT_PHASE=1 node dist/cli/phase1-cli.js metrics",
    "phase1:dashboard": "AI_AGENT_PHASE=1 node dist/cli/phase1-dashboard.js",
    "phase1:demo": "npm run phase1:build && AI_AGENT_PHASE=1 node dist/cli/phase1-cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/barnesy/ai-agent-system.git"
  },
  "keywords": [
    "ai",
    "agents",
    "automation",
    "workflow"
  ],
  "author": "barnesy",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.57.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/x-charts": "^8.9.0",
    "@types/commander": "^2.12.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/uuid": "^10.0.0",
    "boxen": "^5.1.2",
    "chalk": "^4.1.2",
    "commander": "^14.0.0",
    "d3": "^7.9.0",
    "dotenv": "^17.2.1",
    "ora": "^5.4.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "reactflow": "^11.11.4",
    "recharts": "^2.13.3",
    "uuid": "^10.0.0"
  }
}
